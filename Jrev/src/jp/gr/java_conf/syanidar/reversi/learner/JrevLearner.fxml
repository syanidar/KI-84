         <children>
            <TextField fx:id="textField" prefHeight="25.0" prefWidth="170.0" />
            <Button mnemonicParsing="false" onAction="#onLoadButtonClicked" text="Load" />
            <Button fx:id="startButton" mnemonicParsing="false" onAction="#onStartButtonClicked" text="Start" />
         </children>
         <opaqueInsets>
            <Insets />
         </opaqueInsets>
      </HBox>
      <Separator prefWidth="300.0" />
      <HBox alignment="CENTER" prefHeight="25.0" prefWidth="300.0">
         <children>
            <ProgressBar fx:id="progressBar" minHeight="-Infinity" minWidth="-Infinity" prefHeight="15.0" prefWidth="180.0" progress="0.0" />
            <Label fx:id="progressIndicator" alignment="CENTER" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="25.0" prefWidth="80.0" />
         </children>
      </HBox>
